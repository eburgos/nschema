enum <%= name %> {
<%
    var $cnt = 0,
        $currentVal;
    for(var prop in properties) {
        if (typeof(properties[prop].typescriptValue) !== 'undefined') {
            $currentVal = properties[prop].typescriptValue;
        }
        else {
            $currentVal = $cnt;
        }
    %>
    /*
     * <%- (properties[prop].description || '').replace(/\n/g, '\n     * ') %>
     */
    <%- properties[prop].typescriptName ||  $nschema.utils.initialCaps(prop) %> = <%= $currentVal %>,
    <% $cnt += 1; } %>
}
/*::
export type <%= name %> = <%- Object.keys(properties).map(function (prop) { return '"' + prop + '"'; }).join(' | ' ) %>;
*/