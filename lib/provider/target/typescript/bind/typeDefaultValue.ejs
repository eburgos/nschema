<%
    if (!$nschemaType) {
        throw new Error('$nschemaType context variable must be present. If you referenced this inside a loop make sure that variable is set.');
    }
    (function () {
        var $_result;
        var $_typeMap = function (t) {
            switch (t) {
                case 'int': return '0';
                case 'long': return '0';
                case 'float': return '0';
                case 'bool': return 'false';
                case 'string': return 'null';
                case 'date': return 'new Date()';
            }
            return 'null';
        };
        var $_modifierMap = function (t, r) {
            switch (t) {
                case 'list': return '[]';
                case 'array': return '[]';
                case 'option': return 'null';
            }
            return 'null';
        };
        var $_isString = function (t) {
            if ($property.type === 'string') {
                return true;
            }
            else if (typeof($property.type) === 'object') {
                if (($property.type.namespace === '') && ($property.type.name === 'string') && (!$property.type.modifier)) {
                    return true;
                }
            }
            return false;
        };
        if ((typeof($property) !== 'undefined') && (typeof($property.defaultValue) !== 'undefined')) {
            $_result = $property.defaultValue;
            if ($_isString($nschemaType)) {
                $_result = '\"' + $_result + '\"';
            }
        }
        else if ((typeof($property) !== 'undefined') && ($property.init)) {
            $_result = '(new (' + $typescript.typeName($nschemaType, $nschema, namespace, name, $context) + ')())';
        }
        else if ((typeof($nschemaType)) === 'string') {
            $_result = $_typeMap($nschemaType);
        }
        else if (typeof($nschemaType) === 'object') {
            if ($nschemaType.modifier) {
                var $modifier = $nschemaType.typescriptModifier || $nschemaType.modifier;
                if (typeof($modifier) === 'string') {
                    $_result = $_modifierMap($modifier, $typescript.typeName($nschemaType, $nschema, namespace, name, $context));
                }
                else {
                    $_result = $_modifierMap($modifier[$modifier.length - 1], $typescript.typeName($nschemaType, $nschema, namespace, name, $context));
                }
            }
            else {
                $_result = 'null';
            }
        } else {
            $_result = $_typeMap('string');
        }
%><%- $_result %><% })(); %>